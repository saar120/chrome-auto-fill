{
    "manifest_version": 3,
    "name": "AutoFiller 9000",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs",
      "webNavigation"
    ],
    "host_permissions": ["*://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon48.png"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  }
  